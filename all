#include "grade_school.h"


void init_roster(roster_t *p_roster)
{
   p_roster->count = 0;
}

bool add_student(roster_t *p_roster, char *name, int grade)
{
   if (p_roster->count > MAX_STUDENTS)
   {
      return false;
   }
   student_t new_student = { .name = {*name}, .grade = grade };
   p_roster->students[p_roster->count] = new_student;
   ++p_roster->count;
   return true;
}
